{
    "name": "minhbang/laravel-authority",
    "description": "Laravel Authorization Package",
    "keywords": [
        "minhbang",
        "laravel-authority"
    ],
    "homepage": "https://github.com/minhbang/laravel-authority",
    "license": "MIT",
    "authors": [
        {
            "name": "Minh Bang",
            "email": "contact@minhbang.com",
            "homepage": "http://minhbang.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Minhbang\\Authority\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Minhbang\\Authority\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Minhbang\\Authority\\ServiceProvider"
            ]
        }
    }
}
